<h1>Sign Up!</h1>
<%=form_for @user do |form|%>
<p>First Name: <%=form.text_field :f_name %></p>
<p>Last Name: <%=form.text_field :l_name %></p>
<p>Email: <%=form.text_field :email %></p>
<p>Password: <%=form.password_field :password %></p>
<p>Password Confirmation: <%=form.password_field :password_confirmation %></p>
<p><%=form.submit "Create Account" %></p>
<% end %>

